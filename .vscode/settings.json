{
  "[javascript]": {
    "editor.defaultFormatter": "numso.prettier-standard-vscode"
  },
  "[vue]": {
    "editor.defaultFormatter": "numso.prettier-standard-vscode"
  },
  "cSpell.enabled": true,
  "cSpell.words": [
    "aabb",
    "aframe",
    "algo",
    "aliyuncs",
    "Buccolingual",
    "bvhcast",
    "calced",
    "calcing",
    "Cascader",
    "clientset",
    "clim",
    "cmdata",
    "colormap",
    "esbuild",
    "fflate",
    "Formater",
    "Formaters",
    "gaussians",
    "halfedge",
    "halfedges",
    "hibribond",
    "iconfont",
    "Incisal",
    "Interproximal",
    "kdtree",
    "kdtrees",
    "keyname",
    "Labiolingual",
    "lerp",
    "libtess",
    "lightgreen",
    "Linewidth",
    "maxdeep",
    "Mesiodistal",
    "metalness",
    "micromessenger",
    "msgtype",
    "nrrd",
    "Nutstore",
    "OMRCO",
    "ossutil",
    "Overjet",
    "pinia",
    "popconfirm",
    "qrcode",
    "qriously",
    "raycast",
    "raycasted",
    "raycaster",
    "renderstyle",
    "renderthreshold",
    "rhash",
    "rmtr",
    "ryanaltair",
    "sculptable",
    "Shapecast",
    "shnh",
    "slerp",
    "spee",
    "tock",
    "toolkits",
    "truetype",
    "uuidv",
    "viridis",
    "vitejs",
    "vitest",
    "vnode",
    "voronoi",
    "Voxel",
    "weibo",
    "wireframe",
    "yamajik"
  ],
  "debug.javascript.debugByLinkOptions": "always",
  "editor.formatOnSave": true,
  "javascript.validate.enable": false,
  "standard.autoFixOnSave": true,
  "standard.enable": true,
  "standard.options": {
    "global": [
      "Blob",
      "fetch",
      "postMessage",
      "atob",
      "btoa",
      "defineProps",
      "defineExpose",
      "defineEmits",
      "FileReader"
    ],
    "ignore": [
      "node_modules/**",
      "*.vue"
    ]
  },
  "vue3snippets.enable-compile-vue-file-on-did-save-code": false
}
